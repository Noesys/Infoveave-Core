  "version": "2.1.2000",
  "dependencies": {
    "Infoveave.AdapterFramework": "2.0.0-*",
    "Infoveave.Adapters.MicrosoftSQL": "2.0.0-*",
    "Infoveave.Adapters.MondrianService": "2.0.0-*",
    "Infoveave.CacheProvider": "2.0.0-*",
    "Infoveave.Data": "2.0.0-*",
    "Infoveave.FluentDateTime": "2.0.0-*",
    "Infoveave.Mailer": "2.0.0-*",
    "Infoveave.Models": "2.0.0-*",
    "Microsoft.ApplicationInsights.AspNetCore": "1.0.0",
    "Microsoft.AspNetCore.Authentication.JwtBearer": "1.0.0",
    "Microsoft.AspNetCore.Diagnostics": "1.0.0",
    "Microsoft.AspNetCore.Http.Extensions": "1.0.0",
    "Microsoft.AspNetCore.Mvc": "1.0.0",
    "Microsoft.AspNetCore.Owin": "1.0.0",
    "Microsoft.AspNetCore.Razor.Tools": {
      "version": "1.0.0-preview2-final",
      "type": "build"
    },
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.0.0",
    "Microsoft.Extensions.Configuration.Json": "1.0.0",
    "Microsoft.Extensions.FileProviders.Physical": "1.0.0",
    "Microsoft.Extensions.Logging": "1.0.0",
    "Microsoft.Extensions.Logging.Console": "1.0.0",
    "Microsoft.Extensions.Logging.Debug": "1.0.0",
    "Microsoft.Extensions.Options": "1.0.0",
    "Microsoft.Extensions.Options.ConfigurationExtensions": "1.0.0",
    "Newtonsoft.Json": "9.0.1",
    "Serilog": "2.0.0",
    "Serilog.Extensions.Logging": "1.0.0",
    "Serilog.Sinks.RollingFile": "2.0.0",
    "Swashbuckle.SwaggerGen": "6.0.0-beta901",
    "System.IdentityModel.Tokens.Jwt": "5.0.0"
  },

  "tools": {
    "Microsoft.AspNetCore.Razor.Tools": {
      "version": "1.0.0-preview2-final",
      "imports": "portable-net45+win8+dnxcore50"
    },
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": {
      "version": "1.0.0-preview2-final",
      "imports": "portable-net45+win8+dnxcore50"
    }
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "dnxcore50",
        "portable-net45+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0",
          "type": "platform"
        },
        "System.Runtime": "4.1.0",
        "Microsoft.Net.Http": "2.2.29",
        "System.Xml.ReaderWriter": "4.0.11",
        "System.Security.Cryptography.Csp": "4.0.0"
      }
    }
  },

  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true,
    "xmlDoc": true
  },

  "runtimeOptions": {
    "gcServer": true
  },

  "publishOptions": {
    "include": [
      "wwwroot",
      "Views",
      "securityParams.json",
      "settings.production.json",
      "web.config",
      "permission.json",
      "googleAnalytics.json"
    ]
  },

  "scripts": {
    "precompile": [],
    "prepublish": [ "gulp bump" ],
    "postpublish": [ "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%" ]
  }
}
